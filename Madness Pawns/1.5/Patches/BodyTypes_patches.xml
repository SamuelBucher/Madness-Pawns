<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationRemove">
		<xpath>Defs/BodyTypeDef[
		defName="Male" or
		defName="Female" or
		defName="Thin" or
		defName="Hulk" or
		defName="Fat"
		]/woundAnchors/li[tag="RightEye" or tag="LeftEye"]		</xpath>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyTypeDef[
		defName="Male" or
		defName="Female" or
		defName="Thin" or
		defName="Hulk" or
		defName="Fat"
		]/woundAnchors		</xpath>
		<value>
			<li>
				<rotation>South</rotation>
				<tag>RightEye</tag>
				<canMirror>false</canMirror>
				<offset>(-0.097, 0, 0.23)</offset>
				<range>0</range>
				<layer>Head</layer>
				<debugColor>(1,1,1,1)</debugColor>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyTypeDef[
		defName="Male" or
		defName="Female" or
		defName="Thin" or
		defName="Hulk" or
		defName="Fat"
		]/woundAnchors		</xpath>
		<value>
			<li>
				<rotation>South</rotation>
				<tag>LeftEye</tag>
				<canMirror>false</canMirror>
				<offset>(0.097, 0, 0.23)</offset>
				<range>0</range>
				<layer>Head</layer>
				<debugColor>(0,0,0,1)</debugColor>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyTypeDef[
		defName="Male" or
		defName="Female" or
		defName="Thin" or
		defName="Hulk" or
		defName="Fat"
		]/woundAnchors		</xpath>
		<value>
			<li>
				<rotation>East</rotation>
				<tag>RightEye</tag>
				<canMirror>false</canMirror>
				<offset>(0.05, 0, 0.25)</offset>
				<range>0</range>
				<layer>Head</layer>
				<debugColor>(1,1,1,1)</debugColor>
				<narrowCrown>true</narrowCrown>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyTypeDef[
		defName="Male" or
		defName="Female" or
		defName="Thin" or
		defName="Hulk" or
		defName="Fat"
		]/woundAnchors		</xpath>
		<value>
			<li>
				<rotation>East</rotation>
				<tag>LeftEye</tag>
				<canMirror>false</canMirror>
				<offset>(0.22, 0, 0.25)</offset>
				<range>0</range>
				<layer>Head</layer>
				<debugColor>(1,1,1,1)</debugColor>
				<narrowCrown>true</narrowCrown>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyTypeDef[
		defName="Male" or
		defName="Female" or
		defName="Thin" or
		defName="Hulk" or
		defName="Fat"
		]/woundAnchors		</xpath>
		<value>
			<li>
				<rotation>West</rotation>
				<tag>RightEye</tag>
				<canMirror>false</canMirror>
				<offset>(-0.22, 0, 0.25)</offset>
				<range>0</range>
				<layer>Head</layer>
				<debugColor>(1,1,1,1)</debugColor>
				<narrowCrown>true</narrowCrown>
			</li>
		</value>
	</Operation>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyTypeDef[
		defName="Male" or
		defName="Female" or
		defName="Thin" or
		defName="Hulk" or
		defName="Fat"
		]/woundAnchors		</xpath>
		<value>
			<li>
				<rotation>West</rotation>
				<tag>LeftEye</tag>
				<canMirror>false</canMirror>
				<offset>(-0.05, 0, 0.25)</offset>
				<range>0</range>
				<layer>Head</layer>
				<debugColor>(1,1,1,1)</debugColor>
				<narrowCrown>true</narrowCrown>
			</li>
		</value>
	</Operation>
		
</Patch>