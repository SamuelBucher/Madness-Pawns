<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Biotech</li>
		</mods>
		<match Class="PatchOperationSequence">
		   <operations>
			<li Class="PatchOperationRemove">
			  <xpath>Defs/GeneDef[defName="Nose_Pig"]/graphicData/narrowCrownHorizontalOffset</xpath>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/GeneDef[defName="Eyes_Gray"]/graphicData/drawScale</xpath>
				<value>
					<drawScale>0.3</drawScale>				
				</value>
			</li>
			<li Class="PatchOperationReplace">
				<xpath>Defs/GeneDef[defName="Eyes_Red"]/graphicData/drawScale</xpath>
				<value>
					<drawScale>0.3</drawScale>				
				</value>
			</li>
		  </operations>
		</match>
	</Operation>
</Patch>